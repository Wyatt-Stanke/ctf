# NimbusOps Status Dashboard - Environment Configuration
# Copy to .env and fill in values before deploying

# Container registry credentials
DEPLOY_KEY=change-me-before-deploying
REGISTRY_TOKEN=change-me-before-deploying

# Notification webhook
SLACK_WEBHOOK=https://hooks.slack.com/services/XXXXX/XXXXX/XXXXX

# Cluster config
KUBE_CONTEXT=nimbus-prod
KUBE_NAMESPACE=status-dash

# Build config
NODE_ENV=production
